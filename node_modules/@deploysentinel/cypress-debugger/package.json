{
  "name": "@deploysentinel/cypress-debugger",
  "license": "UNLICENSED",
  "version": "0.8.1",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "author": {
    "name": "DeploySentinel"
  },
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "postbuild": "./bin/release-script.sh",
    "ci:unit": "jest --coverage",
    "ci:lint": "yarn lint && yarn tsc --noEmit",
    "dev:unit": "jest --watchAll",
    "prettier": "prettier --config .prettierrc --write .",
    "lint": "eslint . --ext .ts",
    "dev": "rollup -c rollup.config.js -w"
  },
  "dependencies": {
    "@cypress/commit-info": "^2.2.0",
    "@deploysentinel/cypress-runner": "^3.3.0",
    "@deploysentinel/debugger-core": "^0.3.5",
    "ansi-styles": "^6.1.0",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "chrome-remote-interface": "^0.31.3",
    "cypress": "^12.5.1",
    "fast-safe-stringify": "^2.1.1",
    "fast-xml-parser": "^4.2.4",
    "fflate": "^0.7.4",
    "human-interval": "^2.0.1",
    "lodash": "^4.17.21",
    "minimatch": "^5.1.1",
    "semver": "^7.3.8",
    "strip-ansi": "^6.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.2.1",
    "@types/chrome-remote-interface": "^0.31.7",
    "cypress": "^12.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "rollup-plugin-typescript2": "0.33.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  },
  "bin": {
    "ds": "./bin/cli"
  },
  "keywords": [
    "debug",
    "parallel",
    "reporting",
    "dashboard",
    "automation",
    "browser",
    "cypress",
    "plugin",
    "runner",
    "test",
    "testing",
    "e2e",
    "end-to-end"
  ]
}
